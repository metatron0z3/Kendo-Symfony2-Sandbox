<?php

namespace SDI\ChameleonBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TitleGenres
 *
 * @ORM\Table(name="title_genres")
 * @ORM\Entity
 */
class TitleGenres
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="client_genre_id", type="integer", nullable=true)
     */
    private $clientGenreId;

    /**
     * @var string
     *
     * @ORM\Column(name="status", type="string", length=20, nullable=true)
     */
    private $status;

    /**
     * @var \SDI\ChameleonBundle\Entity\Titles
     *
     * @ORM\ManyToOne(targetEntity="SDI\ChameleonBundle\Entity\Titles")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="title_id", referencedColumnName="id")
     * })
     */
    private $title;


}
